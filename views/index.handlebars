<div id="home">
  <div class="container overlay-color">
    <div class="row">
      <div class="col-12">
        {{!-- <h1>{{msg}}</h1> --}}
        {{!-- <div class="text-center"><img src="/img/Tempo_logo.png" height="300"><p>KEEPING IT UP WITH IT.</p></div> --}}
        <div class="jumbotron my-0 overlay-white">

          <div class="row mb-5">
            <div class="col-12">
              <h1 class="display-3">Featured Artist</h1>
            </div>
          </div>
    
            </div>
            <div class="container  overlay-color">
              <div class="row mt-0">
                <div class="col-12 text-center my-0">
                  <div class="jumbotron my-0 pt-3 overlay-white">
                    <h1 class="display-2 band-name">{{bandName}}</h1>
                    <hr>
                    <div class="col-6 float-left">
                      <img src="{{bandPhotoURL}}" width="600" class="img-fluid border border-secondary">
                      <button id="tour-dates-btn" type="button" class="btn btn-lg btn-block btn-success my-4"
                        data-dismiss="modal">Tour
                        Dates</button>
                    </div>
                    <div class="col-6 float-right">
                      <p class="text-justify band-info"><strong>Hometown:</strong> {{bandHometown}}</p>
                      <p class="text-justify band-info"><strong>Genre:</strong> {{bandGenre}}</p>
                      <p class="text-justify band-info"><strong>Bio:</strong> {{bandBio}}</p>
                    </div>
                    <div class="clearfix"></div>
                    <div class="col-12 my-4">
                      <hr>
                      <h1 class="display-4 band-name">Discography</h1>
                    </div>
          
                    <div class="col-12 text-left">
                      <table class="table table-striped">
                        <thead>
                          <tr>
                            <th scope="col">Album Name</th>
                            <th scope="col">Album Year</th>
                            <th scope="col">Number of Tracks</th>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each albums}}
                          <tr>
                            <td>{{this.discTitle}}</td>
                            <td>{{this.discYear}}</td>
                            <td>{{this.discTracks}}</td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                    </div>
          
                  </div>
                </div>
          
              </div>
            </div>
          
          </div>
